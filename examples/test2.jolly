
#############################
function TwoSetsParams

    takes Boolean s

    if s
        takes Number n
    else
        takes String n
    end if

    takes Auto d

    Print("s=")
    Println(s)
    Print("n=")
    Println(n)
    Print("d=")
    Println(d)

end TwoSetsParams

#############################

#############################
TwoSetsParams(True, 5, 5)
TwoSetsParams(True, "Hello", "Hello")